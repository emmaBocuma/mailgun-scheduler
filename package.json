{
  "name": "mailgun-scheduler",
  "version": "1.0.1",
  "description": "A Node.js library for sending scheduled sequential emails with Mailgun",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/emmaBocuma/mailgun-scheduler"
  },
  "scripts": {
    "test": "jest --watchAll",
    "prepublishOnly": "npm run build",
    "build": "npm run build:clean && tsc",
    "build:clean": "rimraf lib",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix src --ext .js,.jsx,.ts,.tsx"
  },
  "author": "Emma Williams",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "mailgun-js": "^0.22.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/mailgun-js": "^0.22.10",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.4.2",
    "jest-watch-typeahead": "^0.6.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  }
}
